	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>printspec</servlet-name>
		<servlet-class>com.hemometal.invoice.mgmt.PrintSpec</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>printspec</servlet-name>
		<url-pattern>/printspec</url-pattern>
	</servlet-mapping>
	
