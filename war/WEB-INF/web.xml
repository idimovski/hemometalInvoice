	<servlet-mapping>
		<servlet-name>Hemometalinvoce</servlet-name>
		<url-pattern>/hemometalinvoce</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>getallpro</welcome-file>
	</welcome-file-list>
</web-app>