		<servlet-name>getpro</servlet-name>
		<servlet-class>com.hemometal.invoice.mgmt.GetPro</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getpro</servlet-name>
		<url-pattern>/getpro</url-pattern>
	</servlet-mapping>

	<servlet>
